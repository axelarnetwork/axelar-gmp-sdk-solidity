{
  "extends": "solhint:recommended",
  "plugins": [],
  "rules": {
    "quotes": ["error", "single"],
    "compiler-version": ["off"],
    "func-visibility": [
      "warn",
      {
        "ignoreConstructors": true
      }
    ],
    "no-inline-assembly": "off",
    "no-empty-blocks": "off",
    "avoid-low-level-calls": "off",
    "not-rely-on-time": "off",
    "no-complex-fallback": "off",
    "immutable-vars-naming": "off"
  }
}
